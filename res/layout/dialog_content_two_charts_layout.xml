<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="#003399">
    <LinearLayout 
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:orientation="horizontal"
        android:gravity="center_horizontal"
        android:layout_marginLeft="10dip"
        android:visibility="gone">
        <TextView 
            android:layout_height="wrap_content"
            android:layout_width="60dip"
            android:text="采样点数"/>
        <Spinner 
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:entries="@array/check_data_type"/>
        <TextView 
            android:layout_height="wrap_content"
            android:layout_width="60dip"
            android:text="采样点数"
            android:layout_marginLeft="20dip"/>
        <Spinner 
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:entries="@array/check_data_type"/>
    </LinearLayout>
    <LinearLayout 
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:background="#ffffff"
        android:layout_marginTop="5dip" >
	    	<TextView 
	        	android:id="@+id/dialog_thr_chart_first_title"
	        	android:layout_height="wrap_content"
	        	android:layout_width="match_parent"
	        	android:text="X"
	        	android:layout_marginLeft="15dip"
	        	android:layout_marginTop="2dip"
	        	android:textColor="#000000"/>
        <RelativeLayout 
	    	android:layout_height="wrap_content"
	    	android:layout_width="match_parent">
	    	<LinearLayout 
	        	android:id="@+id/dialog_thr_chart_first_chart"
	        	android:layout_height="80dip"
	        	android:layout_width="210dip"
	        	android:orientation="vertical"></LinearLayout>
	    	<TextView 
	        	android:id="@+id/dialog_thr_chart_first_option_name1"
	        	android:layout_height="wrap_content"
	        	android:layout_width="50dip"
	        	android:text="峰值"
	        	android:layout_toRightOf="@id/dialog_thr_chart_first_chart"
	        	android:layout_marginLeft="5dip"
	        	android:textColor="#000000"/>
	    	<EditText 
	        	android:id="@+id/dialog_thr_chart_first_option_content1"
	        	android:layout_height="wrap_content"
	        	android:layout_width="50dip"
	        	android:layout_toRightOf="@id/dialog_thr_chart_first_option_name1"
	        	android:background="@drawable/blue_narrow_border_edittext_shape"
	        	android:text="1.85"
	        	android:padding="2dip"
	        	android:textColor="#000000"
	        	android:gravity="center"/>
	    	<TextView 
	        	android:id="@+id/dialog_thr_chart_first_option_name2"
	        	android:layout_height="wrap_content"
	        	android:layout_width="50dip"
	        	android:text="峰峰值"
	        	android:layout_toRightOf="@id/dialog_thr_chart_first_chart"
	        	android:layout_below="@id/dialog_thr_chart_first_option_name1"
	        	android:layout_marginTop="8dip"
	        	android:layout_marginLeft="5dip"
	        	android:textColor="#000000"/>
	    	<EditText 
	        	android:id="@+id/dialog_thr_chart_first_option_content2"
	        	android:layout_height="wrap_content"
	        	android:layout_width="50dip"
	        	android:layout_toRightOf="@id/dialog_thr_chart_first_option_name2"
	        	android:layout_below="@id/dialog_thr_chart_first_option_name1"
	        	android:background="@drawable/blue_narrow_border_edittext_shape"
	        	android:layout_marginTop="8dip"
	        	android:text="1.85"
	        	android:padding="2dip"
	        	android:textColor="#000000"
	        	android:gravity="center"/>
	    	<TextView 
	        	android:id="@+id/dialog_thr_chart_first_option_name3"
	        	android:layout_height="wrap_content"
	        	android:layout_width="50dip"
	        	android:text="有效值"
	        	android:layout_toRightOf="@id/dialog_thr_chart_first_chart"
	        	android:layout_below="@id/dialog_thr_chart_first_option_name2"
	        	android:layout_marginTop="8dip"
	        	android:layout_marginLeft="5dip"
	        	android:textColor="#000000"/>
	    	<EditText 
	        	android:id="@+id/dialog_thr_chart_first_option_content3"
	        	android:layout_height="wrap_content"
	        	android:layout_width="50dip"
	        	android:layout_toRightOf="@id/dialog_thr_chart_first_option_name3"
	        	android:layout_below="@id/dialog_thr_chart_first_option_name2"
	        	android:background="@drawable/blue_narrow_border_edittext_shape"
	        	android:layout_marginTop="8dip"
	        	android:text="1.85"
	        	android:padding="2dip"
	        	android:textColor="#000000"
	        	android:gravity="center"/>
		</RelativeLayout>
	    	<TextView 
	        	android:layout_height="wrap_content"
	        	android:layout_width="match_parent"
	        	android:text="Y"
	        	android:layout_marginLeft="15dip"
	        	android:layout_marginTop="2dip"
	        	android:textColor="#000000"/>
	    	<LinearLayout 
	    	    android:layout_height="wrap_content"
	    	    android:layout_width="match_parent"
	    	    android:orientation="horizontal">
	    	    <LinearLayout  
	    	        android:id="@+id/dialog_thr_chart2_sec_chart"
	        		android:layout_height="85dip"
	        		android:layout_width="210dip"
	        		android:layout_below="@id/dialog_thr_chart_first_title"
	        		android:orientation="vertical"></LinearLayout>
	    	    <LinearLayout 
	    	        android:layout_height="fill_parent"
	    	        android:layout_width="wrap_content"
	    	        android:orientation="vertical"
	    	        android:layout_marginLeft="5dip">
	    	        <TextView 
	    	            android:layout_height="wrap_content"
	    	            android:layout_width="wrap_content"
	    	            android:text="频率"
	    	            android:textColor="#000000"/>
	    	        <EditText 
	    	            android:layout_height="wrap_content"
	    	            android:layout_width="wrap_content"
	    	            android:text="1.85"
	    	            android:background="@drawable/blue_narrow_border_edittext_shape"
	    	            android:layout_marginTop="2dip"
	    	            android:textColor="#000000"/>
	    	        <EditText 
	    	            android:layout_height="wrap_content"
	    	            android:layout_width="wrap_content"
	    	            android:text="1.85"
	    	            android:background="@drawable/blue_narrow_border_edittext_shape"
	    	            android:layout_marginTop="2dip"
	    	            android:textColor="#000000"/>
	    	        <EditText 
	    	            android:layout_height="wrap_content"
	    	            android:layout_width="wrap_content"
	    	            android:text="1.85"
	    	            android:background="@drawable/blue_narrow_border_edittext_shape"
	    	            android:layout_marginTop="2dip"
	    	            android:textColor="#000000"/>
	    	    </LinearLayout>
	    	    <LinearLayout 
	    	        android:layout_height="fill_parent"
	    	        android:layout_width="wrap_content"
	    	        android:orientation="vertical"
	    	        android:layout_marginLeft="5dip">
	    	        <TextView 
	    	            android:layout_height="wrap_content"
	    	            android:layout_width="wrap_content"
	    	            android:text="幅值"
	    	            android:textColor="#000000"/>
	    	        <EditText 
	    	            android:layout_height="wrap_content"
	    	            android:layout_width="wrap_content"
	    	            android:text="1.85"
	    	            android:background="@drawable/blue_narrow_border_edittext_shape"
	    	            android:layout_marginTop="2dip"
	    	            android:textColor="#000000"/>
	    	        <EditText 
	    	            android:layout_height="wrap_content"
	    	            android:layout_width="wrap_content"
	    	            android:text="1.85"
	    	            android:background="@drawable/blue_narrow_border_edittext_shape"
	    	            android:layout_marginTop="2dip"
	    	            android:textColor="#000000"/>
	    	        <EditText 
	    	            android:layout_height="wrap_content"
	    	            android:layout_width="wrap_content"
	    	            android:text="1.85"
	    	            android:background="@drawable/blue_narrow_border_edittext_shape"
	    	            android:layout_marginTop="2dip"
	    	            android:textColor="#000000"/>
	    	    </LinearLayout>
	    	</LinearLayout>	
    </LinearLayout>
</LinearLayout>